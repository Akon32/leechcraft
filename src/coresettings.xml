<?xml version="1.0" encoding="UTF-8"?>
<settings>
	<page>
		<label lang="en" value="Appearance" />
		<label lang="ru" value="Внешний вид" />
		<label lang="uk" value="Зовнішній вигляд" />
		<tab>
			<label lang="en" value="Appearance" />
			<label lang="ru" value="Внешний вид" />
            <label lang="uk" value="Зовнішній вигляд" />
			<item type="checkbox" property="UseTabScrollButtons" state="on">
				<label lang="en" value="Use tab scroll buttons" />
				<label lang="ru" value="Использовать кнопки прокрутки табов" />
				<label lang="uk" value="Використовувати кнопки прокручування табів" />
			</item>
			<item type="checkbox" property="ShowMenuBarAsButton" state="off">
				<label lang="en" value="Show menu bar as button" />
				<label lang="ru" value="Показывать панель меню в виде кнопки" />
			</item>
			<item type="checkbox" property="TrySmarterTabsWidth" state="on">
				<label lang="en" value="Try smarter tabs width calculation" />
				<label lang="ru" value="Пытаться использовать более умный алгоритм расчета ширины вкладок" />
			</item>
			<item type="combobox" property="ToolButtonStyle">
				<label lang="en" value="Tool button style:" />
				<label lang="ru" value="Стиль панелей инструментов:" />
				<option name="iconOnly" default="true">
					<label lang="en" value="Only display the icon" />
					<label lang="ru" value="Показывать только иконку" />
				</option>
				<option name="textOnly">
					<label lang="en" value="Only display the text" />
					<label lang="ru" value="Показывать только текст" />
				</option>
				<option name="textBesideIcon">
					<label lang="en" value="The text appears beside the icon" />
					<label lang="ru" value="Текст рядом с иконкой" />
				</option>
				<option name="textUnderIcon">
					<label lang="en" value="The text appears under the icon" />
					<label lang="ru" value="Текст под иконкой" />
				</option>
			</item>
			<item type="combobox" property="TabBarLocation">
				<label lang="en" value="Tab bar location:" />
				<label lang="ru" value="Положение вкладок:" />
				<option name="north" default="true">
					<label lang="en" value="Top" />
					<label lang="ru" value="Сверху" />
				</option>
				<option name="south">
					<label lang="en" value="Bottom" />
					<label lang="ru" value="Снизу" />
				</option>
				<option name="west">
					<label lang="en" value="Left" />
					<label lang="ru" value="Слева" />
				</option>
				<option name="east">
					<label lang="en" value="Right" />
					<label lang="ru" value="Справа" />
				</option>
			</item>
			<item type="customwidget" name="IconSet">
				<label lang="en" value="Icon set:" />
				<label lang="ru" value="Набор иконок:" />
				<label lang="uk" value="Набір іконок:" />
			</item>
			<item type="customwidget" name="AppQStyle">
				<label lang="en" value="Style:" />
				<label lang="ru" value="Стиль:" />
				<label lang="uk" value="Стиль:" />
			</item>
			<item type="combobox" property="Language">
				<label lang="en" value="Language:" />
				<label lang="ru" value="Язык:" />
				<label lang="uk" value="Язик:" />
				<option name="system" default="true">
					<label lang="en" value="System" />
				</option>
				<option name="ar">
					<label lang="en" value="Arabic (العربية)" />
				</option>
				<option name="en">
					<label lang="en" value="English" />
				</option>
				<option name="es">
					<label lang="en" value="Esperanto" />
				</option>
				<option name="it">
					<label lang="en" value="Italian (Italiano)" />
				</option>
				<option name="pl">
					<label lang="en" value="Polish (Polski)" />
				</option>
				<option name="ru_RU">
					<label lang="en" value="Russian (Русский)" />
				</option>
				<option name="uk_UA">
					<label lang="en" value="Ukrainian (Українська)" />
				</option>
			</item>
			<item type="spinbox" property="MaxLogLines" default="500" step="250" minimum="10" maximum="50000">
				<label lang="en" value="Maximum log lines:" />
				<label lang="ru" value="Максимум строк в логе:" />
				<label lang="uk" value="Максимум строк в логу:" />
			</item>
		</tab>
		<tab>
			<label lang="en" value="Shortcuts" />
			<label lang="ru" value="Горячие клавиши" />
			<label lang="uk" value="Гарячі клавиші" />
			<item type="customwidget" label="own" name="ShortcutManager" />
		</tab>
	</page>
	<page>
		<label lang="en" value="Behavior" />
		<label lang="ru" value="Поведение" />
		<label lang="uk" value="Поведінка" />
		<tab>
			<label lang="en" value="Behavior" />
			<label lang="ru" value="Поведение" />
			<label lang="uk" value="Поведінка" />
			<groupbox>
				<label lang="en" value="Behavior" />
				<label lang="ru" value="Поведение" />
				<label lang="uk" value="Поведінка" />
				<item type="checkbox" property="WatchClipboard" state="off">
					<label lang="en" value="Watch clipboard" />
					<label lang="ru" value="Следить за буфером обмена" />
					<label lang="uk" value="Спостерігати за буфером обміну" />
				</item>
				<item type="checkbox" property="ConfirmQuit" default="true">
					<label lang="en" value="Ask quit confirmation" />
					<label lang="ru" value="Запрашивать подтверждение выхода" />
					<label lang="uk" value="Запитувати підтвердження виходу" />
				</item>
				<item type="checkbox" property="ExitOnClose" default="false">
					<label lang="en" value="Exit on close" />
					<label lang="ru" value="Выходить при закрытии" />
				</item>
				<item type="checkbox" property="OpenTabNext" default="false">
					<label lang="en" value="Open new tab next to current" />
					<label lang="ru" value="Открывать вкладку после текущей" />
				</item>
				<item type="checkbox" property="DontAskWhenSingle" default="false">
					<label lang="en" value="Automatically accept task when there is only one handler" />
					<label lang="ru" value="Автоматически добавлять задачу, когда есть только один обработчик" />
				</item>
				<item type="path" property="WatchDirectory" onCancel="clear">
					<label lang="en" value="Watch directory:" />
					<label lang="ru" value="Следить за директорией:" />
					<label lang="uk" value="Слідкувати за каталогом:" />
				</item>
			</groupbox>
			<item type="groupbox" property="ShowPluginMenuInTabs" state="on" checkable="true">
				<label lang="en" value="Show plugin-dependent actions in tab bar context menu" />
				<label lang="ru" value="Показывать зависимые от плагина действия в контекстном меню таббара" />
				<item type="checkbox" property="ShowPluginMenuInTabsAsSubmenu" default="off">
					<label lang="en" value="Show as a submenu" />
					<label lang="ru" value="Показывать как подменю" />
				</item>
			</item>
			<item type="groupbox" property="ShowFinishedDownloadMessages" state="on" checkable="true">
				<label lang="en" value="Show messages in tray" />
				<label lang="ru" value="Показывать сообщения в трее" />
				<label lang="uk" value="Показувати повідомленя в треї" />
				<item type="spinbox" property="FinishedDownloadMessageTimeout" default="10" minimum="1" maximum="15" step="1">
					<lang value="en">
						<label value="Message timeout:" />
						<suffix value=" s" />
					</lang>
					<lang value="ru">
						<label value="Время показа:" />
						<suffix value=" с" />
					</lang>
					<lang value="uk">
						<label value="Відображати повідомлення:" />
						<suffix value=" с" />
					</lang>
				</item>
			</item>
		</tab>
    </page>
	<page>
		<label lang="en" value="Tags" />
		<label lang="ru" value="Теги" />
		<label lang="uk" value="Теги" />
		<groupbox>
			<label lang="en" value="Tags manager" />
			<label lang="ru" value="Менеджер тегов" />
			<item type="customwidget" label="own" name="TagsViewer" />
		</groupbox>
	</page>
	<page>
		<label lang="en" value="Network" />
		<label lang="ru" value="Сеть" />
		<label lang="uk" value="Мережеа" />
		<item type="spinbox" property="CacheSize" default="200" minimum="0" maximum="1048576">
			<lang value="en">
				<label value="Cache size:" />
				<suffix value=" MB" />
			</lang>
			<lang value="ru">
				<label value="Размер кэша:" />
				<suffix value=" МБ" />
			</lang>
			<lang value="uk">
				<label value="Розмір кешу:" />
				<suffix value=" МБ" />
			</lang>
		</item>
		<item type="checkbox" property="FilterTrackingCookies" default="on">
			<label lang="en" value="Filter tracking cookies" />
			<label lang="ru" value="Фильтровать отслеживающие куки" />
		</item>
		<item type="pushbutton" name="ClearCache">
			<label lang="en" value="Clear cache" />
			<label lang="ru" value="Очистить кеш" />
		</item>
		<item type="pushbutton" name="ClearCookies">
			<label lang="en" value="Clear cookies" />
			<label lang="ru" value="Очистить куки" />
		</item>
		<item type="groupbox" property="ProxyEnabled" checkable="true" state="off">
			<label lang="en" value="Application-wide proxy" />
			<label lang="ru" value="Глобальный прокси" />
			<label lang="uk" value="Глобальний проксі" />
			<item type="lineedit" property="ProxyHost">
				<label lang="en" value="Host:" />
				<label lang="ru" value="Хост:" />
				<label lang="uk" value="Хост:" />
			</item>
			<item type="spinbox" property="ProxyPort" default="3128" minimum="1" maximum="65535" step="1">
				<label lang="en" value="Port:" />
				<label lang="ru" value="Порт:" />
				<label lang="uk" value="Порт:" />
			</item>
			<item type="lineedit" property="ProxyLogin">
				<label lang="en" value="Login:" />
				<label lang="ru" value="Логин:" />
				<label lang="uk" value="Логін:" />
			</item>
			<item type="lineedit" property="ProxyPassword" password="true">
				<label lang="en" value="Password:" />
				<label lang="ru" value="Пароль:" />
				<label lang="uk" value="Пароль:" />
			</item>
			<item type="combobox" property="ProxyType">
				<label lang="en" value="Proxy type:" />
				<label lang="ru" value="Тип прокси:" />
				<label lang="uk" value="Тип проксі:" />
				<option name="socks5" default="true">
					<label lang="en" value="SOCKS5" />
				</option>
				<option name="tphttp">
					<label lang="en" value="Transparent HTTP" />
					<label lang="ru" value="Прозрачный HTTP" />
					<label lang="uk" value="Прозорий HTTP" />
				</option>
				<option name="chttp">
					<label lang="en" value="Caching HTTP" />
					<label lang="ru" value="Кэширующий HTTP" />
					<label lang="uk" value="HTTP з кешуванням" />
				</option>
				<option name="cftp">
					<label lang="en" value="Caching FTP" />
					<label lang="ru" value="Кэширующий FTP" />
					<label lang="uk" value="FTP з кешуванням" />
				</option>
			</item>
		</item>
	</page>
	<page>
		<label lang="en" value="Plugins" />
		<label lang="ru" value="Плагины" />
		<groupbox>
			<label lang="en" value="Plugins manager" />
			<label lang="ru" value="Менеджер плагинов" />
			<item type="customwidget" label="own" name="PluginManager" />
		</groupbox>
	</page>
</settings>
